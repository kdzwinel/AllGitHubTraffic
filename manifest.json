{
    "name": "AllGitHubTraffic",
    "description": "Show traffic data from all your GitHub repositories on one chart.",
    "version": "0.13",
    "background": {
        "scripts": ["background.js"]
    },
    "icons": { "20": "icon-20.png", "128": "icon-128.png" },
    "permissions": [ "https://api.github.com/*", "https://github.com/*", "tabs" ],
    "manifest_version": 2,
    "page_action": {
        "default_icon": {
            "19": "icon-20.png"
        }
    }
}
